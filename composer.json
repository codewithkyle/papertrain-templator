{
  "require": {
    "aelvan/imager": "v2.4.0",
    "craftcms/cms": "^3.4.25",
    "craftcms/redactor": "2.7.1",
    "nystudio107/craft-eagerbeaver": "1.0.4",
    "sebastianlenz/linkfield": "1.0.23",
    "verbb/smith": "1.1.6",
    "verbb/super-table": "2.6.1",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\papertrainmodule\\": "modules/papertrainmodule/src/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
