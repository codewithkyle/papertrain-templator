{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "target": "ES2019",
        "module": "ESNext",
        "lib": ["DOM", "ES2019"],
        "moduleResolution": "Node",
        "jsx": "react",
        "jsxFactory": "h",
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true
    },
    "include": ["./page-builder"]
}
